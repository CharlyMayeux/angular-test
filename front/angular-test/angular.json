{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "default-change-detection": {
      "projectType": "application",
      "root": "projects/default-change-detection",
      "sourceRoot": "projects/default-change-detection/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:application",
          "options": {
            "outputPath": "dist/default-change-detection",
            "index": "projects/default-change-detection/src/index.html",
            "browser": "projects/default-change-detection/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/default-change-detection/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/default-change-detection/src/favicon.ico",
              "projects/default-change-detection/src/assets"
            ],
            "styles": [
              "projects/default-change-detection/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "default-change-detection:build:production"
            },
            "development": {
              "buildTarget": "default-change-detection:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "buildTarget": "default-change-detection:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/default-change-detection/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/default-change-detection/src/favicon.ico",
              "projects/default-change-detection/src/assets"
            ],
            "styles": [
              "projects/default-change-detection/src/styles.scss"
            ],
            "scripts": []
          }
        }
      }
    }
  },
  "schematics": {
    "@schematics/angular:application": {
      "strict": true,
      "style": "scss",
      "skipTests": true,
      "standalone": true,
      "ssr": false
    },
    "@schematics/angular:component": {
      "style": "scss",
      "standalone": true,
      "changeDetection": "OnPush",
      "skipTests": true,
      "inlineStyle": true,
      "inlineTemplate": true
    },
    "@schematics/angular:directive": {
      "skipTests": true
    },
    "@schematics/angular:service": {
      "skipTests": true
    },
    "@schematics/angular:pipe": {
      "skipTests": true
    }
  }
}